// Void function: no return type, implicit void return.
fn do_nothing() {
  42;
}

// Void function with parameters that are used as expression statements.
fn use_params(a: i32, b: i32) {
  a + b;
}

// Void function with explicit return (no value).
fn early_void_return(x: i32) {
  if (x > 0) {
    return;
  }
  x;
}
